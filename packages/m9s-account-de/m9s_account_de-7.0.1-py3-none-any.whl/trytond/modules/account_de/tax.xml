<tryton>
  <data>
        <record model="ir.action.report" id="report_account_tax_code_statement">
            <field name="name">Advance Turnover Tax Return</field>
            <field name="records">listed</field>
            <field name="model">account.tax.code</field>
            <field name="report_name">account.tax.code.statement</field>
            <field name="report">account_de/tax_code_statement.fodt</field>
        </record>
        <record model="ir.action.keyword" id="report_account_tax_code_statement_keyword">
            <field name="keyword">form_print</field>
            <field name="model">account.tax.code,-1</field>
            <field name="action" ref="report_account_tax_code_statement"/>
        </record>

        <record model="ir.action.wizard" id="act_code_tax_open_chart">
            <field name="name">Advance Turnover Tax Return</field>
            <field name="wiz_name">account.tax.code.open_chart</field>
        </record>
        <menuitem
            parent="account.menu_reporting"
            action="act_code_tax_open_chart"
            sequence="45"
            id="menu_code_tax_open_chart"
            icon="tryton-tree"/>
  </data>
</tryton>
