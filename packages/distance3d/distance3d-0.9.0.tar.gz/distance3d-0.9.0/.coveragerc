[run]
branch = True
source = distance3d
omit =
    setup.py
    distance3d/plotting.py

[report]
omit =
    bin/*
    examples/*
    distance3d/plotting.py
    distance3d/test/*
exclude_lines =
    pragma: no cover
    except ImportError:
    if verbose
    if self.verbose
    if __name__ == "__main__"
