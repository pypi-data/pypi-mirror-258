[run]
branch = True
source = slack_message_pipe

[report]
exclude_lines =
    if self.debug:
    pragma: no cover
    raise NotImplementedError
    if __name__ == .__main__.:
ignore_errors = True
omit =
    setup.py
    slack_message_pipe/tests.py
    slack_message_pipe/tests/*
    slack_message_pipe/migrations/*
    slack_message_pipe/fpdf_mod/*
    doc/*
